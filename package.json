    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@fastify/autoload": "^6.0.1",
    "@fastify/cors": "^9.0.0",
    "@mercuriusjs/federation": "^3.0.0",
    "@mercuriusjs/gateway": "^3.0.0",
    "c8": "^10.0.0",
    "desm": "^1.3.0",
